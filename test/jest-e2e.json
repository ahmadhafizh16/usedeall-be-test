{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..", 
  "testEnvironment": "node",
  "testRegex": "src/app/.*/Test/.*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globalSetup": "<rootDir>/test/jest.setup.ts",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  }
}